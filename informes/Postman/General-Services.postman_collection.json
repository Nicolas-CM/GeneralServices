		{
			"name": "AuthRestController",
			"item": [
				{
					"name": "api/auth/login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"if (pm.response.code === 200) {  // Verifica si el c√≥digo de respuesta es 200 OK",
									"    var jsonData = pm.response.json();",
									"",
									"    // Aseg√∫rate de que el token est√° presente en la respuesta",
									"    if (jsonData.token) {",
									"        pm.globals.set(\"jwt_token\", jsonData.token);",
									"        pm.collectionVariables.set(\"jwt_token\", jsonData.token);",
									"        pm.environment.set(\"jwt_token\", jsonData.token);",
									"        console.log(\"Token guardado: \" + jsonData.token);",
									"    } else {",
									"        console.log(\"Token no encontrado en la respuesta.\");",
									"    }",
									"} else {",
									"    console.log(\"Error: Respuesta no v√°lida\");",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\r\n    \"username\": \"admin\",\r\n    \"password\": \"password123\"\r\n}\r\n",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": "{{base_url}}/api/auth/login"
					},
					"response": []
				},
				{
					"name": "api/auth/logout",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// Verifica si el logout fue exitoso y limpia el token\r",
									"if (pm.response.code === 200) {\r",
									"    pm.environment.unset(\"jwt_token\"); // Elimina el token despu√©s del logout\r",
									"    console.log(\"Logout exitoso y token eliminado\");\r",
									"} else {\r",
									"    console.log(\"Error en el logout\");\r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"auth": {
							"type": "bearer",
							"bearer": {
								"token": "{{jwt_token}}"
							}
						},
						"method": "POST",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"username\": \"admin\",\n    \"password\": \"password123\"\n}\n",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": "{{base_url}}/api/auth/logout"
					},
					"response": []
				}
			]
		},
